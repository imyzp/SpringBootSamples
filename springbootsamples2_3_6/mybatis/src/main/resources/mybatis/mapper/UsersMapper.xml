<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yzp.spring.springsamples.mybatis.mapper.UsersMapper">


    <insert id="addUsers" parameterType="com.yzp.spring.springsamples.mybatis.model.Users">
        insert into users(loginname,password)
        values(#{loginName},#{passWord})
    </insert>

</mapper>